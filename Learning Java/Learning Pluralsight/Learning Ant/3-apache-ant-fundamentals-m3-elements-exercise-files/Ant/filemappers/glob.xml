<?xml version="1.0" encoding="UTF-8"?>
<project name="Filemapper glob example" default="glob" basedir=".">
    
    <target name="glob">
        <copy todir="build/glob">
            <mapper type="glob" from="*.java" to="*.java.backup" />
            <fileset dir="src">
                <include name="**/*.java" />
            </fileset>
        </copy>
    </target>
    
</project>
