<?xml version="1.0" encoding="UTF-8"?>
<project name="Filemapper merge example" default="merge" basedir=".">
    
    <target name="merge">
        <copy todir="build/merge">
            <mapper type="merge" to="mge" />
            <fileset dir="src">
                <include name="**/*.java" />
            </fileset>
        </copy>
    </target>
    
</project>
